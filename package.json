{
  "description": "Polymer vo-template component",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vo-template.git"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vo-template#readme",
  "name": "vo-template",
  "version": "1.0.0",
  "directories": {
    "test": "test",
    "demo": "demo"
  },
  "devDependencies": {
    "np": "3.0.4",
    "polymer-cli": "1.8.1",
    "@polymer/iron-demo-helpers": "3.0.2",
    "@polymer/iron-component-page": "4.0.1",
    "@webcomponents/webcomponentsjs": "2.1.3",
    "wct-browser-legacy": "1.0.2"
  },
  "scripts": {
    "clean": "rm -rf build node_modules build bower_components etc node",
    "build": "npm run build:dist",
    "build:docs": "polymer analyze dist/vo-template.js > analysis.json",
    "build:dist": "polymer build",
    "build:local": "npm run clean && bower install && npm install && npm run test && npm run build:dist && npm run build:docs",
    "dev": "browser-sync start --proxy localhost:8080 --startPath / --serveStatic . --files .",
    "test": "polymer test -l chrome,firefox --expanded",
    "test:chrome": "polymer test -l chrome --expanded",
    "test:firefox": "polymer test -l firefox --expanded",
    "release": "np",
    "release:testless": "np --yolo"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vo-template/issues"
  },
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  },
  "main": "vo-template.js",
  "dependencies": {
    "vodomg-litelement": "1.0.0",
    "vo-header": "1.0.3",
    "vo-footer": "1.0.2",
    "vo-font": "1.0.2"
  }
}
